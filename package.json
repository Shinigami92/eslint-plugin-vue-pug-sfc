{
  "name": "eslint-plugin-vue-pug-sfc",
  "version": "1.0.0-alpha.20",
  "description": "eslint plugin vue pug sfc",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -Rf dist pnpm-lock.yaml node_modules",
    "build": "tsc",
    "add:rule": "esno ./scripts/add-rule.ts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "jest",
    "prepublishOnly": "pnpm run clean && pnpm install && pnpm run build"
  },
  "keywords": [
    "pug",
    "vue",
    "vuejs",
    "eslint",
    "eslintplugin",
    "plugin",
    "lint",
    "linter",
    "linting",
    "sfc"
  ],
  "author": {
    "name": "Christopher Quadflieg",
    "email": "chrissi92@hotmail.de",
    "url": "https://github.com/Shinigami92"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shinigami92/eslint-plugin-vue-pug-sfc.git"
  },
  "bugs": "https://github.com/Shinigami92/eslint-plugin-vue-pug-sfc/issues",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "tsconfig.json"
  ],
  "dependencies": {
    "pug-lexer": "^5.0.1",
    "vue-eslint-parser": "^8.3.0"
  },
  "devDependencies": {
    "@types/eslint": "~8.4.1",
    "@types/eslint-scope": "~3.7.3",
    "@types/eslint-visitor-keys": "~1.0.0",
    "@types/jest": "~27.4.1",
    "@types/node": "~16.11.26",
    "@typescript-eslint/eslint-plugin": "~5.13.0",
    "@typescript-eslint/parser": "~5.13.0",
    "eslint": "~8.10.0",
    "eslint-config-prettier": "~8.4.0",
    "eslint-define-config": "~1.2.5",
    "eslint-gitignore": "~0.1.0",
    "eslint-plugin-prettier": "~4.0.0",
    "esno": "~0.14.1",
    "jest": "~27.5.1",
    "prettier": "2.5.1",
    "prettier-plugin-organize-imports": "~2.3.4",
    "ts-jest": "~27.1.3",
    "typescript": "~4.6.2"
  },
  "packageManager": "pnpm@6.32.2",
  "engines": {
    "node": ">= 16.9.0",
    "npm": ">= 8.5.3",
    "pnpm": ">= 6.32.2"
  }
}
